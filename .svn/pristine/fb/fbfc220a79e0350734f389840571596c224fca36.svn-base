<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="create_run_jar" name="Create Runnable Jar for Project RPG">
    <!--this file was created by Eclipse Runnable JAR Export Wizard-->
    <!--ANT 1.7 is required                                        -->
    <!--define folder properties-->
    <property name="dir.buildfile" value="."/>
    <property name="dir.workspace" value="/home/finsky/finsky/mytemp/ohj2/ws"/>
    <property name="dir.jarfile" value="${dir.buildfile}"/>
    <target name="create_run_jar">
        <jar destfile="${dir.jarfile}/Rpg.jar" filesetmanifest="mergewithoutmain">
            <manifest>
                <attribute name="Main-Class" value="fxRpg.RpgMain"/>
                <attribute name="Class-Path" value="."/>
            </manifest>
            <fileset dir="/home/finsky/ht/bin"/>
            <zipfileset excludes="META-INF/*.SF" src="/home/finsky/finsky/devel/jar/Ali.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="/home/finsky/finsky/devel/jar/fxgui.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="/home/finsky/finsky/devel/jar/Graphics.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="/home/finsky/finsky/devel/jar/Music.jar"/>
        </jar>
    </target>
</project>
